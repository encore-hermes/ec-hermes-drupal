<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
	<rule>
		<from>\.(php|gif|css|jpg|png|ico|js|html|htm|txt)</from>
		<to last="true">.$1</to>
	</rule>

	<rule>
		<from>^/(.*)</from>
		<to>/index.php?q=$1</to>
	</rule>
</urlrewrite>